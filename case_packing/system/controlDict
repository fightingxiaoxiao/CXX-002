FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Make sure all utilities know specialised models
libs            (atmosphericModels);

application     MPPICFoamEx;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         5;

deltaT          0.0002;

writeControl    timeStep;

writeInterval   500;

purgeWrite      0;

writeFormat     binary;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

maxAlphaCo      1.0;

functions
{
    #include "sampleDict"
    //#include "sampleIso"
    //#include "samplePlanes"

    minMax
    {
        type    fieldMinMax;
        libs    (fieldFunctionObjects);

        writeControl    writeTime;
        fields          (U.air);
    }
}


// ************************************************************************* //
