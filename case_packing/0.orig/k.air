FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(inlet|outlet|front|back)"
    {
        type            cyclic;
    }

    ground
    {
        type            kqRWallFunction;
        value           uniform 0.0;
    }

    top
    {
        type            zeroGradient;
    }

    sides
    {
        type            symmetry;
    }
}


