FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.3";
    object      rho.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];


internalField   uniform 1.2;

boundaryField
{
    top
    {
        type            calculated;
        value           uniform 1.2;
    }
    ground
    {
        type            calculated;
        value           uniform 1.2;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
}


